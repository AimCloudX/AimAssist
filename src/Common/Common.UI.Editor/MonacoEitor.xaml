<UserControl x:Class="Common.UI.Editor.MonacoEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Common.UI.Editor" 
             xmlns:wpf="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
             Background="Transparent"
             >
    <Grid Background="Transparent">
<Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal">
        </StackPanel>
    <wpf:WebView2 Grid.Row="1" DefaultBackgroundColor="Transparent" x:Name="webView" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" ></wpf:WebView2>
    </Grid>
</UserControl>

